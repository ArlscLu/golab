    "version": "0.2.0",
    "configurations": [
        {
            "name": "Default",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {},
            "args": []
        },
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program":"${workspaceFolder}/tstd/tnet/main.go",
            "env":{"ENV": "PROD","ENV2": "PROD","ENV3":11},
            // "args": ["-delta=7h,8s,9s -d=123 -t"],
            "args": ["-age=19"],
            "showLog": false
        },
        {
            "name": "Test file",
            "type": "go",
            "request": "launch",
            "mode": "test",
            //In test or debug mode, this refers to the absolute path to the package or file to debug. 
            //In exec mode, this is the existing binary file to debug. Not applicable to attach requests.
            "program":"/usr/local/var/code/golab/std/flag_test.go",
            "env":{"ENV": "PROD","ENV2": "PROD","ENV3":11},
            "args": ["-test.v"],
            "showLog": false
        }
    ]
}